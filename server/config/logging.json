{
  "appenders": [
    {
      "type": "file",
      "filename": "logs/express_access.log",
      "maxLogSize": 5000000,
      "backups": 3,
      "category": "express_access"
    },
    {
      "type": "console",
      "category": "DEV_express_access"
    },
    {
      "type": "file",
      "filename": "logs/app.log",
      "maxLogSize": 5000000,
      "backups": 3,
      "category": "app"
    },
    {
      "type": "console",
      "category": "DEV_app"
    }
  ],
  "levels": {
    "express_access": "INFO",
    "DEV_express_access": "DEBUG",
    "app": "INFO",
    "DEV_app": "DEBUG"
  }
}